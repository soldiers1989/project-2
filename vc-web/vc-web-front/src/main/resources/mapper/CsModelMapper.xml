<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ccclubs.admin.orm.mapper.CsModelMapper">
	<!-- 开启基于redis的二级缓存，仅适用单表
    <cache type="com.ccclubs.framework.mybatis.RedisCache"/>-->
    
	<resultMap id="BaseResultMap" type="com.ccclubs.admin.model.CsModel">
		<result column="csm_id" jdbcType="INTEGER" property="csmId" />
		<result column="csm_name" jdbcType="VARCHAR" property="csmName" />
		<result column="csm_flag" jdbcType="VARCHAR" property="csmFlag" />
		<result column="csm_type" jdbcType="TINYINT" property="csmType" />
		<result column="csm_file" jdbcType="TINYINT" property="csmFile" />
		<result column="csm_catagory" jdbcType="TINYINT" property="csmCatagory" />
		<result column="csm_tank_capacity" jdbcType="DECIMAL" property="csmTankCapacity" />
		<result column="csm_battery_capacity" jdbcType="DECIMAL" property="csmBatteryCapacity" />
		<result column="csm_status" jdbcType="TINYINT" property="csmStatus" />
		<result column="csm_add_time" jdbcType="TIMESTAMP" property="csmAddTime" />
		<result column="csm_update_time" jdbcType="TIMESTAMP" property="csmUpdateTime" />
	</resultMap>
	
	
</mapper>